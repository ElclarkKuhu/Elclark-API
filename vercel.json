{
	"redirects": [
		{
			"source": "/",
			"destination": "https://elclark.my.id/"
		}
	],
	"rewrites": [
		{
			"source": "/auth",
			"destination": "/api/auth"
		},
		{
			"source": "/file",
			"destination": "/api/file"
		},
		{
			"source": "/file/url",
			"destination": "/api/file/url"
		},
		{
			"source": "/files",
			"destination": "/api/files"
		},
		{
			"source": "/spotify/token.json",
			"destination": "/api/spotify/token.json"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Credentials",
					"value": "true"
				},
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "GET, OPTIONS, PATCH, DELETE, POST, PUT"
				},
				{
					"key": "Access-Control-Allow-Headers",
					"value": "X-CSRF-Token, X-Requested-With, Authorization, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
				}
			]
		}
	]
}
